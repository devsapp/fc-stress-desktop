{
  "name": "fc-stress",
  "version": "0.0.7",
  "description": "This is a component demo for Serverless Devs Tool ",
  "keywords": [
    "Serverless",
    "Framework",
    "Serverless-Devs",
    "Serverless-Tool",
    "Function Compute"
  ],
  "homepage": "https://www.serverless-devs.com",
  "author": "Serverless-Devs",
  "contributors": [],
  "license": "MIT",
  "main": "./lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ServerlessTool/express-alibaba"
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "npm run doc && tsc -w",
    "publish": "npm run build && s cli platform publish",
    "build": "rm -r node_modules && rm package-lock.json && npm i && tsc",
    "postbuild": "npm run doc && mkdir -p lib/lib/utils/stress_test && cd src/lib/utils/stress_test && zip -r -q ../../../../lib/lib/utils/stress_test/code.zip ./",
    "doc": "npx typedoc src/index.ts --json doc/doc.json --out doc"
  },
  "dependencies": {
    "@alicloud/fc2": "^2.2.2",
    "@serverless-devs/core": "^0.0.*",
    "archiver": "^5.3.0",
    "colors": "^1.4.0",
    "fs-extra": "^10.0.0",
    "i18n": "^0.13.2",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.21",
    "lodash.get": "^4.4.2",
    "progress": "^2.0.3",
    "rimraf": "^3.0.2",
    "silly-datetime": "^0.1.2",
    "try-require": "^1.2.1",
    "tty-table": "^4.1.3",
    "promise-retry": "^2.0.1",
    "@alicloud/ram": "^1.0.0"
  },
  "devDependencies": {
    "@types/rimraf": "^3.0.0",
    "@types/fs-extra": "^9.0.11",
    "@types/lodash": "^4.14.170",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "eslint": "^7.7.0",
    "ts-node": "^8.10.2",
    "typedoc": "^0.20.35",
    "typescript": "^3.9.7"
  }
}
